@model AspCoreSD.Models.Contadorescliente

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<h4>Contadorescliente</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdCliente" class="control-label"></label>
                <input asp-for="IdCliente" class="form-control" />
                <span asp-validation-for="IdCliente" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdContador" class="control-label"></label>
                <input asp-for="IdContador" class="form-control" />
                <span asp-validation-for="IdContador" class="text-danger"></span>
            </div>
            <div class="form-row">
                <div class="form-group col-md-6">
                    <input type="submit" value="Crear" class="btn btn-primary btn-block" />
                </div>

                <div class="form-group col-md-6">
                    <a asp-action="Index" class="btn btn-default btn-block"><i class="fa fa-table"></i>Regresar</a>
                </div>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
